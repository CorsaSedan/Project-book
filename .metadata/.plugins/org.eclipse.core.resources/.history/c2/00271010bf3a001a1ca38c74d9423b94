package com.project.main;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.ComponentScan;

@SpringBootApplication
@ComponentScan(basePackageClasses=com.project.controller.ContatoController.class)
@ComponentScan(basePackageClasses = com.project.dao.ContatoDAO.class)
public class ProjectMysqlApplication {

	public static void main(String[] args) {
		SpringApplication.run(ProjectMysqlApplication.class, args);
	}

}
